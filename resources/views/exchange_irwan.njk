                                            t += '<div class="settingsRow rowSettings">';
                                            t += '<ul class="button-group" id="buttons' + id + '">';
                                            for (var i = 0; i < rows.length; i++) {
                                                var r = rows[i];
                                                var pct = (100 / rows.length) + '%';
                                                t += '<li style="width: ' + pct + ';"><button class="button show show' + r + '">' + r + '</button></li>';
                                            }
                                            t += '</ul>';
                                            t += '<div class="desc">' + id + ' Height:</div>';
                                            t += '</div>';
                                        }
                                        return t;
                                    }

                                    function initTrollBox() {

                                        $('#TrollboxContainer .help').click(function() {
