                                                    var link_link = "<a class=\"standard\" href=\"" + link_text + "\">" + link_text + "</a>";
                                                    trollboxRow['message'] = trollboxRow['message'].replace(link_text, link_link);
                                                }
                                            }

                                            if (trollboxRowContent.indexOf('color:') == -1) {
                                                repScale = Math.floor((trollboxRow['reputation'] / maxReputation) * 10);
                                                if (repScale > 10) repScale = 10;
                                                repScale = repScale.toString();
                                                if (repScale.length < 2) repScale = '0' + repScale;
                                                trollboxRowContent += '<span class="reputation r' + repScale + '">';
                                            }
                                            trollboxRowContent += trollboxRow['username'] + ": ";
                                            trollboxRowContent += '</span>';
                                            trollboxRowContent += '</a>';
                                            trollboxRowContent += '</strong>';

                                            if (trollboxRow['message'].substring(0, 6) == "/tiny ") {
                                                trollboxRowContent += '<span class="tiny">';
                                                trollboxRow['message'] = trollboxRow['message'].substring(6);
                                                tiny = true;
                                            }

                                            if (trollboxRow['username'] == "Banhammer") trollboxRowContent += '<strong>';
                                            if (trollboxRow['message'].indexOf(trollboxUsername) != -1 && trollboxUsername != null) trollboxRowContent += '<span class="mention">';
                                            //if (trollboxRow['username'] == "SeriousCoin14" && trollboxRow['message']==":itshappening:")trollboxRow['message']='<img src="/images/itshappening.gif" width="130" height="86"></img>';
                                            //if (trollboxRow['message']==":itsatrap:")trollboxRow['message']='<img src="/images/itsatrap.jpg" width="108" height="140"></img>';
                                            trollboxRowContent += trollboxRow['message'];
                                            if (trollboxRow['message'].indexOf(trollboxUsername) != -1 && trollboxUsername != null) trollboxRowContent += '</span>';
                                            if (trollboxRow['username'] == "Banhammer") trollboxRowContent += '</strong>';
                                            if (trollboxRow['username'] == "Banhammer") trollboxRowContent += '</span><br><div style="width:100%; text-align:center;"><img src="' + themeFolder + '/banhammer.gif" width="62" height="62"></img></div>';

                                            if (tiny) trollboxRowContent += '</span>';

                                        }
                                        return trollboxRowContent;
                                    }

                                    function scrubMessage(message) {
                                        message = message.replace(/'/g, "&#8217;");
                                        message = message.replace(/"/g, "&quot;");
                                        message = message.replace(/\(/g, "&#40;");
                                        message = message.replace(/\)/g, "&#41;");
                                        message = message.replace(/@/g, "&#64;");
                                        message = message.replace(/[^A-Za-z0-9\.\!\?\/\+\;\:\=\,\%\&\#\_ -]/g, '');
                                        var curseArray = ['shit', 'fuck', 'fuk', 'cunt', 'nigger', 'fagot', 'faggot', 'fag'];
                                        for (x = 0; x < curseArray.length; x++) {
                                            message = message.replace(new RegExp(curseArray[x], "gi"), "*bleep*");
                                        }
                                        return message;
                                    }

                                    function reinitTrollboxPane() {
                                        $('#trollboxScrollArea').data('jsp').reinitialise();
                                    }

                                    function refreshTrollbox(data, complete, skipOwn) {
                                        if (muteTrollbox == true) return;
                                        var scrollToBottom = false;
                                        if (($("#trollboxScrollArea")[0].scrollHeight - $("#trollboxScrollArea").scrollTop()) <= $("#trollboxScrollArea").outerHeight()) {
                                            scrollToBottom = true;
                                        }

                                        var newMessages = data;
                                        var sortedKeys = Object.keys(newMessages).sort();
                                        var trollboxRow;
                                        var trollboxContent = '';
                                        var trollboxRowContent = '';
                                        var table = document.getElementById("trollboxTable");
                                        for (var x = 0; x < sortedKeys.length; x++) {
                                            messageNumber = sortedKeys[x];
                                            if (document.getElementById(messageNumber) == null) {
                                                var skipAll = false;
                                                if (parseInt(messageNumber) > currentMessageNumber) {
                                                    currentMessageNumber = parseInt(messageNumber);
                                                } else {
                                                    skipAll = true;
                                                }
                                                trollboxRow = newMessages[messageNumber];
                                                if (trollboxRow['username'] == trollboxUsername) ownReputation = trollboxRow['reputation'];
                                                if ((trollboxRow['username'] == trollboxUsername | (trollboxRow['username'] == 'busoni@gibrex' && trollboxUsername == 'busoni&#64;gibrex')) && complete == false && (skipMessage | skipOwn)) {
                                                    if (!skipOwn) skipMessage = false;
                                                    skippedMessageNumbers.push(messageNumber);
                                                    skippedMessages.push(trollboxRow['message']);
                                                    continue;
                                                }
                                                sStart = skippedMessages.length - 20;
                                                if (sStart < 0) sStart = 0;
                                                for (xx = sStart; xx < skippedMessages.length; xx++) {
                                                    if (trollboxRow['message'] == skippedMessages[xx]) continue;
                                                }
                                                for (xx = sStart; xx < skippedMessageNumbers.length; xx++) {
                                                    if (messageNumber == skippedMessageNumbers[xx]) continue;
                                                }
                                                if (((trollboxRow['username'] == trollboxUsername | (trollboxRow['username'] == 'busoni@gibrex' && trollboxUsername == 'busoni&#64;gibrex'))) && skipAll) continue;
                                                var row = table.insertRow(-1);
                                                row.id = messageNumber;
                                                var cell = row.insertCell(0);
                                                trollboxRowContent = getTrollboxRow(trollboxRow);
                                                cell.innerHTML = trollboxRowContent;

                                                var count = $('#trollboxTable tr').length;
                                                if (count > 150) table.deleteRow(0);
                                            }
                                        }
                                        refreshingTrollbox = false;
                                        if (scrollToBottom) {
                                            $("#trollboxScrollArea").scrollTop(9999);
                                        }
                                        reinitTrollboxPane();
                                    }

                                    $("#muteTrollboxCheckbox").change(function() {
                                        if ($("#muteTrollboxCheckbox:checked").length > 0) {
                                            muteTrollbox = true;
                                            $("#trollboxTable").empty();
